{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "api/webhook.js"
    }
  ],
  "env": {
    "BOT_TOKEN": "@bot_token",
    "GOOGLE_SERVICE_ACCOUNT_KEY": "@google_service_account_key",
    "GOOGLE_SHEET_ID": "@google_sheet_id",
    "ADMIN_CHAT_ID": "@admin_chat_id"
  }
}
